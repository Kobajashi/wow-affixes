{% macro display(affixes) %}
    {% if affixes is same as("Unknown") %}
        {{ 'homepage.not_defined_yet'|trans }}
    {% else %}
        {% for affix in affixes %}
            <a href="https://www.wowhead.com/{% if app.request.locale != "en" %}{{ app.request.locale }}/{% endif %}affix={{ affix }}/" class="affixes affixes-{{ affix }}"></a><br/>
        {% endfor %}
    {% endif %}
{% endmacro %}
